    "@types/jasmine": "^4.0.3",
    "core-js": "3",
    "msw": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sinon": "^14.0.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
